page {
  height: 100%;
  background: #f5f5f5;
}
.questions {
  height: 100%;
  position: relative;
}
.questions .test-header {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  width: 690rpx;
  padding: 0 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: 34px;
  line-height: 34px;
  border-bottom: solid 1px #ddd;
  background: #f5f5f5;
}
.questions .test-header view {
  color: #555;
  font-size: 14px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.questions .test-header view image {
  margin-top: 10px;
  margin-right: 5px;
  width: 14px;
  height: 14px;
}
.questions .test-header view:nth-child(1) {
  font-size: 12px;
}
.questions .test-header view:nth-child(1) text {
  color: #666;
}
.questions .test-header view:nth-child(3) {
  font-size: 12px;
}
.questions .test-header view:nth-child(3) text {
  color: #666;
}
.questions .questions-cont {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}
.questions .questions-cont .swiper-item {
  width: 100vw;
  min-width: 100vw;
  max-width: 100vw;
  height: 100%;
  overflow: auto;
}
.questions .questions-cont .swiper-item .test-main {
  padding: 0 20rpx;
  margin: 49px 30rpx;
  margin-bottom: 20px;
  background: #fff;
}
.questions .questions-cont .swiper-item .test-main .test-title {
  font-size: 16px;
  color: #333;
  padding: 20rpx 0;
}
.questions .questions-cont .swiper-item .test-main .test-title text {
  font-size: 10px;
  color: #fff;
  background: #aaa;
  padding: 2px 5px;
  margin-right: 5px;
  border-radius: 3px;
}
.questions .questions-cont .swiper-item .test-main .test-title .test-title-img {
  margin: 20rpx auto;
  text-align: center;
}
.questions .questions-cont .swiper-item .test-main .test-title .test-title-img image {
  max-width: 500rpx;
  max-height: 250rpx;
}
.questions .questions-cont .swiper-item .test-main .test-cont {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  padding-bottom: 20rpx;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view {
  min-height: 40px;
  line-height: 40px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view view {
  width: 30px;
  height: 30px;
  line-height: 30px;
  margin-top: 4px;
  margin-right: 7px;
  font-size: 14px;
  border: solid 1px #ddd;
  border-radius: 50%;
  box-shadow: 2px 2px 5px #e5e5e5;
  text-align: center;
  color: #666;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view image {
  max-width: 560rpx;
  max-height: 280rpx;
  margin-bottom: 20rpx;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view text {
  font-size: 13px;
  color: #555;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view.active_true view {
  background: #1a7bb9;
  border: solid 1px #1a7bb9;
  color: #fff;
  font-size: 15px;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view.active_true text {
  color: #1a7bb9;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view.active view {
  background: #FF6667;
  border: solid 1px #FF6667;
  color: #fff;
  font-size: 15px;
}
.questions .questions-cont .swiper-item .test-main .test-cont > view.active text {
  color: #FF6667;
}
.questions .questions-cont .swiper-item .test-btn {
  background: #00aaff;
  font-size: 14px;
  border-radius: 6px;
  text-align: center;
  line-height: 40px;
  color: #fff;
  height: 40px;
  width: 100px;
  margin: 20px auto;
}
.questions .questions-cont .swiper-item .test-describe {
  padding: 0 20rpx;
  margin-bottom: 20px;
  border-top: solid 1px #ccc;
}
.questions .questions-cont .swiper-item .test-describe .describe-title {
  height: 48px;
  line-height: 48px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.questions .questions-cont .swiper-item .test-describe .describe-title text {
  color: #666;
  font-size: 12px;
}
.questions .questions-cont .swiper-item .test-describe .describe-title image {
  width: 14px;
  height: 14px;
  margin-top: 17px;
  margin-left: 3px;
}
.questions .questions-cont .swiper-item .test-describe .describe-cont {
  background: #f5f5f5;
  padding: 12rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.questions .questions-cont .swiper-item .test-describe .describe-cont > text {
  color: #666;
  font-size: 14px;
  line-height: 24px;
}
.questions .questions-cont .swiper-item .test-describe .describe-cont > text:nth-child(3) {
  font-size: 12px;
  line-height: 20px;
}
.questions .fixed-bottom {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  background: rgba(0, 0, 0, 0.4);
  pointer-events: none;
  opacity: 0;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
}
.questions .fixed-bottom .tibiao {
  position: absolute;
  bottom: 0;
  left: 0;
  background: #fff;
  width: 100%;
  overflow: auto;
  height: 50%;
  padding: 30rpx 20rpx;
}
.questions .fixed-bottom .tibiao .tibiao-bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: reverse;
  -webkit-flex-direction: row-reverse;
          flex-direction: row-reverse;
  padding-bottom: 30rpx;
  border-bottom: solid 1px #ddd;
  height: 16px;
  line-height: 16px;
}
.questions .fixed-bottom .tibiao .tibiao-bar view {
  width: 10px;
  height: 10px;
  border-radius: 50%;
  margin-left: 30rpx;
  margin-top: 3px;
}
.questions .fixed-bottom .tibiao .tibiao-bar view.tibiao-yes {
  background: #1a7bb9;
}
.questions .fixed-bottom .tibiao .tibiao-bar view.tibiao-no {
  background: #FF6667;
}
.questions .fixed-bottom .tibiao .tibiao-bar view.tibiao-null {
  background: #999;
}
.questions .fixed-bottom .tibiao .tibiao-bar text {
  margin-left: 3px;
  display: inline-block;
  font-size: 13px;
}
.questions .fixed-bottom .tibiao .tibiao-bar text.tibiao-yes2 {
  color: #1a7bb9;
}
.questions .fixed-bottom .tibiao .tibiao-bar text.tibiao-no2 {
  color: #FF6667;
}
.questions .fixed-bottom .tibiao .tibiao-bar text.tibiao-no3 {
  color: #666;
}
.questions .fixed-bottom .tibiao > view:nth-child(2) text {
  margin-left: 22rpx;
  margin-top: 10rpx;
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50%;
  font-size: 14px;
  color: #666;
  background: #e4e4e4;
}
.questions .fixed-bottom .tibiao > view:nth-child(2) text:nth-child(6n+1) {
  margin-left: 0;
}
.questions .fixed-bottom .tibiao > view:nth-child(2) text.active_o {
  background: #beddf1;
  color: #1a7bb9;
}
.questions .fixed-bottom .tibiao > view:nth-child(2) text.active_t {
  background: #beddf1;
  color: #1a7bb9;
}
.questions .fixed-bottom.active {
  opacity: 1;
  pointer-events: auto;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
}

