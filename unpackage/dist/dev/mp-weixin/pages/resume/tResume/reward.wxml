<view><cu-custom vue-id="ca9f9ee0-1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">奖项证书</view></cu-custom><view class="content bg-white"><view class="tagItemSelectContent"><view><text>已有奖项证书：</text></view><block wx:for="{{selectList}}" wx:for-item="tagText" wx:for-index="idx" wx:key="idx"><view class="tagItemSelect"><view><text data-text="{{tagText}}">{{tagText}}</text></view><image class="select_delete" src="../../../static/remove.png" data-text="{{tagText}}" data-event-opts="{{[['tap',[['removeLabel',['$event']]]]]}}" bindtap="__e"></image></view></block></view><view class="newTagContent flex justify-center"><view class="newTagName"><input class="newTagInput" type="text" placeholder="输入新增标签" placeholder-style="font-size:14px;text-align:center;" data-event-opts="{{[['input',[['__set_model',['','tagName','$event',[]]]]]]}}" value="{{tagName}}" bindinput="__e"/></view><view class="newTagClick"><button class="newTagClickBtn" type="primary" size="default" data-event-opts="{{[['tap',[['createTags',['$event']]]]]}}" bindtap="__e">添加</button></view></view><view class="tagContainer"><view><text>常用奖项证书：</text></view><view class="_br"></view><block wx:for="{{tagList}}" wx:for-item="tagText" wx:for-index="index" wx:key="index"><view class="tagItem"><text data-text="{{tagText}}" data-event-opts="{{[['tap',[['selectLabel',['$event']]]],['longpress',[['showDelLabel',['$event']]]]]}}" bindtap="__e" bindlongpress="__e">{{tagText}}</text><block wx:if="{{tag_del==true}}"><image class="tag_delete" src="../../../static/delete_white.png" data-text="{{tagText}}" data-event-opts="{{[['tap',[['delLabel',['$event']]]]]}}" bindtap="__e"></image></block></view></block><block wx:if="{{tag_del==true}}"><view data-event-opts="{{[['tap',[['cancelDel',['$event']]]]]}}" class="cancelDel" bindtap="__e">取消删除</view></block></view></view><button data-event-opts="{{[['tap',[['store',['$event']]]]]}}" class="cu-btn bg-blue margin-bottom-sm xl justify-center flex" bindtap="__e">保存</button></view>